{
  "name": "agile-react-boiler",
  "private": true,
  "author": "Anshul Vandra (anshul.vandra@agileinfoways.com)",
  "version": "0.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run prettier && vite",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "test": "vitest run && vitest --ui",
    "coverage": "vitest run --coverage ",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write \"**/*.{ts,tsx,css}\" --config ./.prettierrc",
    "prepare": "husky install",
    "server-json": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.48.0",
    "@tanstack/react-query-devtools": "^5.48.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@vitejs/plugin-react": "^4.0.4",
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "json-server": "^1.0.0-beta.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.52.0",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^10.0.5",
    "serve": "^14.2.0",
    "styled-components": "^6.0.8",
    "vite-plugin-pwa": "^0.16.5",
    "vite-tsconfig-paths": "^4.2.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/crypto-js": "^4.2.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.4",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run prettier"
    ],
    "*.{js,css,md,html,json}": "prettier --cache --write"
  }
}
